generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Appointment {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())
  timestamp DateTime @unique
  end       DateTime @unique
}

model Test {
  id   Int    @id @default(autoincrement())
  text String
}
