{
  "name": "appointment-booking-api",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Perry Olsson",
  "license": "MIT",
  "scripts": {
    "tsc": "rm -rf dist/ && tsc",
    "watch": "rm -rf dist/ && tsc --watch",
    "start": "NODE_ENV=production node ./dist/index.js",
    "dev": "NODE_ENV=development ts-node-dev --respawn src/index.ts",
    "dev:js": "NODE_ENV=development nodemon dist/index.js",
    "lint": "eslint ./src/**/*.ts",
    "format": "prisma format",
    "generate": "prisma generate",
    "migrate:init": "rm -rf ./prisma/migrations && prisma migrate dev --name init --preview-feature",
    "migrate:dev": "prisma migrate dev --preview-feature",
    "db:push": "prisma db push"
  },
  "devDependencies": {
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.25",
    "nodemon": "^2.0.7",
    "prisma": "2.16.0",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@prisma/client": "^2.16.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1"
  }
}
